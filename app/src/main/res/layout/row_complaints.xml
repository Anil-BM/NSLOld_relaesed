<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.nsl.beejtantra.commonutils.CustomTextViewNormal
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_13sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:textSize="@dimen/_11sdp"
            android:layout_marginTop="@dimen/_8sdp"
            android:text="@string/name"
            android:textColor="@color/black"
            android:visibility="gone" />


        <com.nsl.beejtantra.commonutils.CustomTextViewNormal
            android:id="@+id/tv_place"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_13sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:textSize="@dimen/_11sdp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="2">

            <com.nsl.beejtantra.commonutils.CustomTextViewNormal
                android:id="@+id/tv_retailername"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_13sdp"
                android:layout_marginTop="@dimen/_3sdp"
                android:layout_marginRight="@dimen/_8sdp"
                android:layout_weight="1"
                android:textAllCaps="true"
                android:text="slkdjflksjf"
                android:textColor="@color/colorPrimary"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold" />

                <com.nsl.beejtantra.commonutils.CustomTextViewLight
                    android:id="@+id/stage_label"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Stage : "
                    android:layout_marginTop="@dimen/_3sdp"
                    android:textSize="@dimen/_10sdp"
                    android:layout_weight="0.3"/>
                <com.nsl.beejtantra.commonutils.CustomTextViewNormal
                    android:id="@+id/tv_stage"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.7"
                    android:layout_marginLeft="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_3sdp"
                    android:layout_marginRight="@dimen/_8sdp"
                    android:textAllCaps="true"
                    android:text="slkdjflksjf"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold" />


            </LinearLayout>

            <ImageView
                android:id="@+id/status_icon"
                android:layout_width="@dimen/_16sdp"
                android:layout_height="@dimen/_12sdp"
                android:layout_marginLeft="@dimen/_13sdp"
                android:layout_marginRight="@dimen/_8sdp"
                android:layout_marginTop="@dimen/_8sdp" />

            <com.nsl.beejtantra.commonutils.CustomTextViewNormal
                android:id="@+id/statusval"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_13sdp"
                android:layout_marginRight="@dimen/_8sdp"
                android:textSize="@dimen/_11sdp"
                android:visibility="gone" />
        </LinearLayout>

<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2"
        android:orientation="horizontal">
        <com.nsl.beejtantra.commonutils.CustomTextViewNormal
            android:id="@+id/tv_complainttype"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginBottom="@dimen/_3sdp"
            android:layout_marginLeft="@dimen/_13sdp"
            android:layout_marginTop="@dimen/_3sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:text="sdjflksdjf"
            android:textColor="@color/textgraycolor"
            android:textSize="@dimen/_11sdp" />
        <com.nsl.beejtantra.commonutils.CustomTextViewLight
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Status : "
            android:layout_marginTop="@dimen/_3sdp"
            android:textSize="@dimen/_10sdp"
            android:layout_weight="0.3"/>
        <com.nsl.beejtantra.commonutils.CustomTextViewNormal
            android:id="@+id/tv_status"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.7"
            android:layout_marginBottom="@dimen/_3sdp"
            android:layout_marginLeft="@dimen/_13sdp"
            android:layout_marginTop="@dimen/_3sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:text="sdjflksdjf"
            android:textColor="@color/textgraycolor"
            android:textSize="@dimen/_11sdp" />


    </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2"
            android:orientation="horizontal">
            <com.nsl.beejtantra.commonutils.CustomTextViewNormal
                android:id="@+id/tv_comments"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginBottom="@dimen/_3sdp"
                android:layout_marginLeft="@dimen/_13sdp"
                android:layout_marginTop="@dimen/_3sdp"
                android:layout_marginRight="@dimen/_8sdp"
                android:text="sdjflksdjf"
                android:textColor="@color/textgraycolor"
                android:textSize="@dimen/_11sdp" />
            <com.nsl.beejtantra.commonutils.CustomTextViewLight
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="Date : "
                android:layout_marginTop="@dimen/_3sdp"
                android:textSize="@dimen/_10sdp"
                android:layout_weight="0.3"/>
            <com.nsl.beejtantra.commonutils.CustomTextViewNormal
                android:id="@+id/tv_date"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.7"
                android:layout_marginBottom="@dimen/_3sdp"
                android:layout_marginLeft="@dimen/_13sdp"
                android:layout_marginTop="@dimen/_3sdp"
                android:layout_marginRight="@dimen/_8sdp"
                android:text="sdjflksdjf"
                android:textColor="@color/textgraycolor"
                android:textSize="@dimen/_11sdp" />


        </LinearLayout>
        <com.nsl.beejtantra.commonutils.CustomTextViewNormal
            android:id="@+id/tv_billno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_13sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:visibility="gone" />
        <LinearLayout
            android:id="@+id/ll_accept_reject"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_30sdp"
            android:layout_margin="@dimen/_5sdp"
            android:orientation="horizontal"
            android:gravity="center"
            android:visibility="gone"
            android:weightSum="3">

<!--
            <View
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />
-->

            <ImageView
                android:id="@+id/tv_reject"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/_8sdp"
                android:background="@drawable/background_orange"
                android:padding="@dimen/_8sdp"
                android:src="@drawable/delete"
                android:visibility="visible" />




            <com.nsl.beejtantra.commonutils.CustomTextViewNormal
                android:id="@+id/tv_accept"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/_8sdp"
                android:layout_weight="1"
                android:background="@drawable/background_green"
                android:gravity="center"
                android:text="APPROVE"
                android:textColor="@color/white"
                android:textSize="@dimen/_11sdp" />

            <com.nsl.beejtantra.ButtonPlus
                android:id="@+id/view_comments"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_gravity="right"
                android:layout_marginLeft="@dimen/_8sdp"
                android:layout_marginRight="@dimen/_8sdp"
                android:layout_weight="1"
                android:background="@drawable/background_orange"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:gravity="center"
                android:text="View"
                android:textAlignment="center"
                android:textAllCaps="true"
                android:textColor="@color/white"
                android:textSize="@dimen/_11sdp"
                android:visibility="gone" />


        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="@dimen/_13sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:background="@color/rowbottomcolor"></View>
    </LinearLayout>

</LinearLayout>